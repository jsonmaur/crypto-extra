{
  "name": "crypto-extra",
  "version": "0.2.0",
  "description": "convenience methods for node crypto",
  "author": "Jason Maurer <j@maur.co>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jsonmaur/node-crypto-extra"
  },
  "keywords": [
    "crypto",
    "cryptography",
    "hash",
    "checksum",
    "encrypt",
    "decrypt",
    "encryption",
    "decryption",
    "sha1",
    "md5",
    "aes256",
    "bcrypt",
    "random",
    "hex",
    "cipher",
    "extra"
  ],
  "files": [
    "lib/",
    "README.md"
  ],
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint src test",
    "pretest": "rm -rf .nyc_output coverage && npm run lint",
    "test": "cross-env NODE_ENV=testing nyc ava -a",
    "posttest": "nyc report -r=lcov",
    "coveralls": "cat coverage/lcov.info | coveralls",
    "build": "rm -rf lib && babel src -d lib -s --no-comments",
    "prepublish": "npm run build"
  },
  "devDependencies": {
    "ava": "0.14.0",
    "babel-cli": "6.7.5",
    "babel-plugin-transform-runtime": "6.7.5",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-stage-0": "6.5.0",
    "coveralls": "2.11.9",
    "cross-env": "1.0.7",
    "eslint": "2.7.0",
    "eslint-config-zab": "0.2.0",
    "nyc": "6.4.0",
    "sinon": "1.17.3"
  },
  "dependencies": {
    "babel-runtime": "6.6.1",
    "bcryptjs": "2.3.0",
    "fs-promise": "0.5.0"
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "include": [
      "src"
    ]
  }
}
