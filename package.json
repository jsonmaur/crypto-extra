{
  "name": "crypto-extra",
  "version": "0.1.5",
  "description": "convenience methods for node crypto",
  "author": "Jason Maurer <j@maur.co>",
  "license": "MIT",
  "homepage": "https://github.com/jsonmaur/node-crypto-extra",
  "repository": {
    "type": "git",
    "url": "https://github.com/jsonmaur/node-crypto-extra"
  },
  "keywords": [
    "crypto",
    "cryptography",
    "hash",
    "sha256",
    "md5",
    "bcrypt",
    "random",
    "hex",
    "encrypt",
    "encryption",
    "key",
    "decrypt",
    "aes256",
    "cipher",
    "extra"
  ],
  "main": "lib/index.js",
  "scripts": {
    "prebuild": "rm -rf lib",
    "build": "babel src -d lib -s --no-comments",
    "lint": "eslint src",
    "pretest": "npm run lint && npm outdated && npm run build",
    "test": "istanbul cover tape -- tests/**/*.js | tap-spec",
    "coveralls": "cat coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "prepublish": "npm run build"
  },
  "devDependencies": {
    "babel-cli": "6.5.1",
    "babel-preset-es2015": "6.5.0",
    "coveralls": "2.11.8",
    "eslint": "2.2.0",
    "eslint-config-xo": "0.11.0",
    "eslint-config-zab": "0.1.0",
    "istanbul": "0.4.2",
    "tap-spec": "4.1.1",
    "tape": "4.4.0"
  },
  "dependencies": {
    "babel-plugin-transform-runtime": "6.5.2",
    "babel-runtime": "6.5.0",
    "bcryptjs": "2.3.0",
    "denodeify": "1.2.1",
    "es6-promise": "3.1.2"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "eslintConfig": {
    "extends": "zab"
  }
}
